<section class="announcement container mx-auto py-8">
    <h1 class="text-2xl font-bold mb-8">Announcements</h1>
    <table class="w-full table-auto bg-white">
        <thead>
            <tr>
                <th class="px-4 py-2 text-left">NO.</th>
                <th class="px-4 py-2 text-left">Title</th>
                <th class="px-4 py-2 text-left">Date</th>
            </tr>
        </thead>
        <tbody>
            <% announcement.forEach(function(announcement) { %>
                <tr>
                    <td class="px-4 py-2 text-left">blah</td>
                    <td class="px-4 py-2 text-left"><a class="announce" href="/announcements/<%= announcement._id %>"><%= announcement.title %></a></td>
                    <td class="px-4 py-2 text-left"><%= moment(announcement.createdAt).format("MM.DD.YYYY") %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</section>